{
  "compilerOptions": {
    "lib": ["DOM.Iterable", "DOM", "ESNext"],
    "module": "ES2020",
    "target": "ES2020",
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "removeComments": true,
    "strict": true
  }
}
